
<div class="row mt-5">
    <div class="col-md-12 text-center">
        <nav aria-label="Page navigation" class="text-center">
            <ul class="pagination">
                {% if page !=1 %}
                <li class="page-item"><a class="page-link" href="{{ page-1 }}">&lt;</a></li>
                {% endif %}
                <li class="page-item active"><a class="page-link " onclick="return false;" href="/{{ page }}">{{ page }}</a></li>
                {% if page != pages %}
                <li class="page-item"><a class="page-link" href="/{{ page+1 }}">&gt;</a></li>
                {% endif %}
            </ul>
        </nav>
    </div>
</div>
